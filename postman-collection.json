                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json",
                                "type": "text"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"file_ids\": [10000, 10001]\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/v1/download/initiate",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "v1",
                                "download",
                                "initiate"
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Check Download Availability",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json",
                                "type": "text"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"file_id\": 10000\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/v1/download/check",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "v1",
                                "download",
                                "check"
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Check Download Availability (Sentry Test)",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json",
                                "type": "text"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"file_id\": 10000\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/v1/download/check?sentry_test=true",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "v1",
                                "download",
                                "check"
                            ],
                            "query": [
                                {
                                    "key": "sentry_test",
                                    "value": "true"
                                }
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Start Download",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json",
                                "type": "text"
